{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style": "off",
        "no-underscore-dangle":"off",
        "max-classes-per-file": "off",
        "lines-between-class-members":"off",
        "class-methods-use-this":"off"
    }
}
